import { useNavigate } from "react-router-dom"

export const useNavigator = () => {
  const navigate = useNavigate();

  const navigateHome = () => {
    return navigate("/")
  }
  const navigateSurvey = () => {
    return navigate("/survey")
  }
  const navigateResult = () => {
    return navigate("/results")
  }

  return {
    navigateHome,
    navigateSurvey,
    navigateResult
  }
}